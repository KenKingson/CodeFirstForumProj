@model CodeFirstForum.ViewModels.CreateViewModel

@foreach (Step item in Model.Steps)
{
    <div class="row">
        <div class="col-md-8">
            <h3 style="display: inline-block; margin-left:10px">@Html.Raw(@item.Title)</h3>
            <hr />
            <h5>@Html.Raw(@item.Content) suck</h5>
        </div>
    </div>
}
